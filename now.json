{
  "version": 2,
  "name": "general-proxy",
  "alias": "proxy.sklinkusch.now.sh",
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    }
  ]
}
